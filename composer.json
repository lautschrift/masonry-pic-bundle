{
    "name": "lautschrift/masonry-pic-bundle",
    "type": "contao-bundle",
    "description": "Masonry Pics",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Lautschrift",
            "homepage": "https://github.com/lautschrift/"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.4.*",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Lautschrift\\MasonryPicBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Lautschrift\\MasonryPicBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lautschrift\\MasonryPicBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/lautschrift/masonry-pic-bundle/issues",
        "source": "https://github.com/lautschrift/masonry-pic-bundle"
    }
}
